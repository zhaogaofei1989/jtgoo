<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cn.jtgoo.cms.domain">

	<class name="ReserveOrder" table="jtgoo_reserve_order">
		<id name="id">
			<generator class="native" />
		</id>
		<property name="product_name" column="product_name" type="string"></property>
		<property name="product_id" column="product_id" type="long"></property>
		<property name="order_price" column="order_price" type="string"></property>
		<property name="customer_name" column="customer_name" type="string"></property>
		<property name="mobile_phone" column="mobile_phone" type="string"></property>
		<property name="mail" column="mail" type="string"></property>
		<property name="city" column="city" type="string"></property>
		<property name="number_of_people1" column="number_of_people1" type="string"></property>
		<property name="number_of_people2" column="number_of_people2" type="string"></property>
		<property name="remark" column="remark" type="string"></property>
		<property name="register_time" column="register_time" type="string"></property>
		<property name="ip_addr" column="ip_addr" type="string"></property>
		<property name="del_status" type="string" column="del_status" length="1"></property>	
	</class>

</hibernate-mapping>



